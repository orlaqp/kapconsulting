<div class="page-header">
  <%= link_to time_sheet_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_time_sheet_path(@time_clock), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show time clock entry</h1>
</div>

<dl class="dl-horizontal">
  <dt>Start time:</dt>
  <dd><%= @time_clock.start_time %></dd>

  <dt>End time:</dt>
  <dd><%= @time_clock.end_time %></dd>

  <dt>Comments:</dt>
  <dd><%= @time_clock.comments %></dd>

</dl>
