<div class="page-header">
  <%= link_to interns_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_intern_path(@intern), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show intern</h1>
</div>

<dl class="dl-horizontal">
  <dt>Department:</dt>
  <dd><%= @intern.department_id %></dd>

  <dt>Term:</dt>
  <dd><%= @intern.term_id %></dd>

  <dt>Position title:</dt>
  <dd><%= @intern.position_title %></dd>

  <dt>First name:</dt>
  <dd><%= @intern.first_name %></dd>

  <dt>Last name:</dt>
  <dd><%= @intern.last_name %></dd>

  <dt>Phone number:</dt>
  <dd><%= @intern.phone_number %></dd>

  <dt>Time zone:</dt>
  <dd><%= @intern.time_zone_id %></dd>

  <dt>Personal email:</dt>
  <dd><%= @intern.personal_email %></dd>

  <dt>School email:</dt>
  <dd><%= @intern.school_email %></dd>

</dl>
